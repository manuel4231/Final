            <string>/// Movimiento del personaje

// Obtener las entradas de teclado

key_right = keyboard_check(vk_right);
key_left = keyboard_check(vk_left);
key_space = keyboard_check_pressed(vk_space);
key_down = keyboard_check(vk_down);

// Operaciones con las entradas de teclado

move = key_right - key_left;
hsp = move * movespeed;

if (vsp &lt; 10) {vsp += grav;}

if (place_meeting(x, y+1, objMuro))
{
    jumps = jumpsmax;
}

if (key_space) &amp;&amp; (jumps &gt; 0)
{
    jumps -= 1;
    vsp = -jumpspeed;
}

if (key_space) &amp;&amp; (place_meeting(x+1,y,objMuro) || place_meeting(x-1,y,objMuro))
{
    vsp = -jumpspeed;
}

var hsp_final = hsp + hsp_carry;
hsp_carry = 0;

// Colision horizontal

if (place_meeting(x+hsp_final, y, objMuro)){
    while (!place_meeting(x+sign(hsp_final), y, objMuro))
    {
        x += sign(hsp_final);
    }
    hsp_final = 0;
    hsp = 0;
}

x += hsp_final;


// Colision vertical

if (place_meeting(x, y+vsp, objMuro))
{
    while (!place_meeting(x, y+sign(vsp), objMuro)){
        y += sign(vsp);
    }
    vsp = 0;
}


y += vsp;

// Animate

if (Animate &lt; 0)
{

if (move!= 0) image_xscale = move;
if (place_meeting(x,y+1,objMuro))
{
    if (hsp!= 0) sprite_index = sprShanaCaminando; else sprite_index = sprShanaQuieta;
}
else
{
    if (vsp &lt;0) sprite_index = sprShanaSaltando;
}
}
else
{
if (move!= 0) image_xscale = move;
if (place_meeting(x,y+1,objMuro))
{
    if (hsp &lt; 0) sprite_index = sprPulpoIzquierda; else sprite_index = sprPulpoDerecha;
}
else
{
    if (vsp &lt; 0) sprite_index = sprPulpoArriba; else sprite_index = sprPulpoAbajo;
}
}

</string>
